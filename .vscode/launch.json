{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "(gdb) bb.bin",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bb.bin",
            "args": [
                "-tmax",
                "3600",
                "-fnmr",
                "data/nmr_spec/testA_chain_A_dmax_5.nmr",
                "-clean_log",
                "-solver",
                "BF",
                "-verbose"
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) tests.bin",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/tests.bin",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/build/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) codes/bb.py",
            "type": "python",
            "request": "launch",
            "program": "codes/bb.py",
            "console": "integratedTerminal",
            "args": [
                "-tmax",
                "3600",
                "-fnmr",
                "profiling/data/4wua.nmr",
                "-clean_log"
            ],
            "justMyCode": true
        },
        {
            "name": "(gdb) codes/tests.py",
            "type": "python",
            "request": "launch",
            "program": "codes/tests.py",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}